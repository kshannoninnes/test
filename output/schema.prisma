datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Book {
  isbn Int @id @default(autoincrement())
  title String
  author Int
}

model Author {
  id Int @id @default(autoincrement())
  firstName String
  lastName String?
}